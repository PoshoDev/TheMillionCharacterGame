        <arguments>
          <argument>
            <kind>1</kind>
            <string>//Tells if the character is not moving
if keyboard_check_released(vk_left)
    move = 0
if keyboard_check_released(vk_right)
    move = 0

//Sets animation speeds
if move = 1
    guy.image_speed =   0.2
else
    guy.image_speed =   0
    
//Checking for turning left or right
if keyboard_check(vk_left)
    if guy.image_xscale &lt; size
        if turn = 0
            turn = 1
if keyboard_check(vk_right)
    if guy.image_xscale &gt; -size
        if turn = 0
            turn = -1
            
//Animation for turning left or right
if turn = -1
    {
    guy.image_xscale -= 1
    if guy.image_xscale = -size
        turn = 0
    }
if turn = 1
    {
    guy.image_xscale += 1
    if guy.image_xscale = size
        turn = 0
    }
//Animation for jumping

if jump = 1
    if vspeed &lt; 0
        if dir = -1
            if guy.image_angle &gt; -18
                guy.image_angle -= 2
if jump = 1
    if vspeed &lt; 0
        if dir = 1
            if guy.image_angle &lt; 18
                guy.image_angle += 2
            
//Animation for falling
if vspeed &gt; 0
    if dir = 1
        if guy.image_angle &gt; -8
            guy.image_angle -= 2
               
if vspeed &gt; 0
    if dir = -1
        if guy.image_angle &lt; 8
            guy.image_angle += 2
    

//Animation for double-jump spin (if active)
if dj_spin = -1
    {
    guy.image_angle += 18
    if guy.image_angle = 360
        {
        guy.image_angle = 0
        dj_spin = 0
        }
    }
if dj_spin = 1
    {
    guy.image_angle -= 18
    if guy.image_angle = -360
        {
        guy.image_angle = 0
        dj_spin = 0
        }
    }
    
//Animation for attacking
if keyboard_check(ord('X'))
    if shoot_able = 1
        if jump = 0
            if shoot_anim = 0
                shoot_anim = 1
                
if shoot_anim = 1   //Pushes front
    guy.image_angle -= 10*dir
if guy.image_angle = -50*dir
    shoot_anim = 2
    
if shoot_anim = 2   //Pushes back
    {
    guy.image_angle += 8.3*dir
    if guy.image_angle = -0.2*dir
        {
        guy.image_angle = 0
        shoot_able = 1
        shoot_anim = 0
        }
    }
</string>
