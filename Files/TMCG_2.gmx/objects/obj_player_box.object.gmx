          <argument>
            <kind>1</kind>
            <string>//Makes the character run faster if a left/right is pressed twice
if jump = 0
{

if keyboard_check_pressed(vk_left)  //LEFT
    if alarm[0] &gt; 0
        if run = 1
            if run_dir = -1
                {
                run = 2
                instance_create(x,y+size*8, obj_part_dust)
                }
        
if keyboard_check_released(vk_left)
    if run = 0
        {
        alarm[0] = 10   //NEEDS MORE BALANCING!!!!!
        run = 1
        run_dir = dir
        }
    else
        {
        run = 0
        alarm[0] = 0
        }
        
if keyboard_check_pressed(vk_right)  //RIGHT
    if alarm[0] &gt; 0
        if run = 1
            if run_dir = 1
                {
                run = 2
                instance_create(x,y+size*8, obj_part_dust)
                }
        
if keyboard_check_released(vk_right)
    if run = 0
        {
        alarm[0] = 10   //NEEDS MORE BALANCING!!!!!
        run = 1
        run_dir = dir
        }
    else
        {
        run = 0
        alarm[0] = 0
        }
        
}
        
if run = 2  //The rest
    total_speed =   8
else
    total_speed =   4
</string>
